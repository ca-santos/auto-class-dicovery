{
  "name": "caue-santos/auto-class-discovery",
  "description": "Discover classes and informations about them",
  "license": "MIT",
  "keywords": [
    "auto",
    "class",
    "discovery"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Caue Santos",
      "email": "cauesantosre4@gmail.com"
    }
  ],
  "homepage": "https://github.com/caueSantos/auto-class-dicovery",
  "require": {
    "php": "^7.2|^8.1",
    "illuminate/support": "^8.0|^9.0|^10.0"
  },
  "require-dev": {
    "orchestra/testbench": "^4.0|^5.0|^6.0",
    "phpunit/phpunit": "^8.4|^9.0"
  },
  "autoload": {
    "psr-4": {
      "CaueSantos\\AutoClassDiscovery\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CaueSantos\\AutoClassDiscovery\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "CaueSantos\\AutoClassDiscovery\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
